<div class="container">
{{# each phones}}
    <div class="row equal">
    {{# each this}}
  <div class="col-sm-6 col-md-4 equal">
    <div class="thumbnail">
      <img src="{{this.imagePath}}">
      <div class="caption">
        <h3>{{this.title}}</h3>
        <h4>{{this.Type}}</h4>
        <p>{{this.description}}</p>
        <div class="price pull-left">&#8377 {{this.price}}</div> 
        <a href="/add-to-cart/{{this._id}}" class="btn btn-success pull-right" role="button">Add to cart</a></p>
      </div>
    </div>
  </div>
  {{/each}}
</div>
{{/each}}
</div>

